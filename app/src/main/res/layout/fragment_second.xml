<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_frame2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.swashconvergence.apps.user.Fragment_Company.SecondFragment">
    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
<LinearLayout
    android:padding="12dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="Applicant Company Profile"
        android:textSize="16dp"
        android:layout_marginTop="20dp"
        android:fontFamily="sans-serif"
        android:textColor="@color/colorPrimaryDark"/>

    <android.support.v7.widget.AppCompatEditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:hint="Name of the Company"
        android:layout_marginTop="7dp"
        android:drawablePadding="16dp"
        android:fontFamily="san-serif"
        android:inputType="text"
        android:background="@drawable/edt_bg_selector"
        android:textColorHint="@color/grey"
        android:singleLine="true"
        android:textSize="@dimen/text_head"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Registered Address"
            android:textSize="@dimen/text_head"
            android:fontFamily="sans-serif"
            android:textColor="@color/grey"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Add New Address"
            android:id="@+id/add_new_address"
            android:layout_alignParentRight="true"
            android:textSize="@dimen/text_head"
            android:fontFamily="sans-serif"
            android:textColor="@color/grey"
            />

    </RelativeLayout>
    <android.support.v7.widget.RecyclerView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/recycler_view">

    </android.support.v7.widget.RecyclerView>
    <android.support.v7.widget.AppCompatEditText
        android:id="@+id/incpdate"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/edt_bg_selector"
        android:hint="Incorporated On"
        android:layout_marginTop="7dp"
        android:drawablePadding="16dp"
        android:fontFamily="san-serif"
        android:inputType="text"
        android:textColorHint="@color/grey"
        android:singleLine="true"
        android:textSize="@dimen/text_head"/>
    <android.support.v7.widget.AppCompatEditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:hint="Phone"
        android:background="@drawable/edt_bg_selector"
        android:layout_marginTop="7dp"
        android:drawablePadding="16dp"
        android:fontFamily="san-serif"
        android:inputType="phone"
        android:textColorHint="@color/grey"
        android:singleLine="true"
        android:textSize="@dimen/text_head"/>
    <android.support.v7.widget.AppCompatEditText
        android:layout_width="match_parent"
        android:id="@+id/phone"
        android:layout_height="50dp"
        android:hint="Cellular"
        android:background="@drawable/edt_bg_selector"
        android:layout_marginTop="7dp"
        android:drawablePadding="16dp"
        android:fontFamily="san-serif"
        android:inputType="phone"
        android:textColorHint="@color/grey"
        android:singleLine="true"
        android:textSize="@dimen/text_head"/>
    <android.support.v7.widget.AppCompatEditText
        android:layout_width="match_parent"
        android:id="@+id/email"
        android:layout_height="50dp"
        android:hint="Email ID"
        android:background="@drawable/edt_bg_selector"
        android:layout_marginTop="7dp"
        android:drawablePadding="16dp"
        android:fontFamily="san-serif"
        android:inputType="textEmailAddress"
        android:textColorHint="@color/grey"
        android:singleLine="true"
        android:textSize="@dimen/text_head"/>
    <android.support.v7.widget.AppCompatEditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/edt_bg_selector"
        android:hint="No of Board of Directors"
        android:layout_marginTop="7dp"
        android:drawablePadding="16dp"
        android:fontFamily="san-serif"
        android:inputType="number"
        android:textColorHint="@color/grey"
        android:singleLine="true"
        android:textSize="@dimen/text_head"/>
    <android.support.v7.widget.AppCompatEditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:hint="Website"
        android:background="@drawable/edt_bg_selector"
        android:layout_marginTop="7dp"
        android:drawablePadding="16dp"
        android:fontFamily="san-serif"
        android:inputType="text"
        android:textColorHint="@color/grey"
        android:singleLine="true"
        android:textSize="@dimen/text_head"/>
    <android.support.v7.widget.AppCompatEditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:hint="No. of Employee"
        android:layout_marginTop="7dp"
        android:background="@drawable/edt_bg_selector"
        android:drawablePadding="16dp"
        android:fontFamily="san-serif"
        android:inputType="number"
        android:textColorHint="@color/grey"
        android:singleLine="true"
        android:textSize="@dimen/text_head"/>
    <android.support.v7.widget.AppCompatEditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:hint="Pan No."
        android:layout_marginTop="7dp"
        android:drawablePadding="16dp"
        android:fontFamily="san-serif"
        android:inputType="text"
        android:textColorHint="@color/grey"
        android:singleLine="true"
        android:background="@drawable/edt_bg_selector"
        android:textSize="@dimen/text_head"/>
    <android.support.v7.widget.AppCompatEditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:hint="Annual Turnover"
        android:background="@drawable/edt_bg_selector"
        android:layout_marginTop="7dp"
        android:drawablePadding="16dp"
        android:fontFamily="san-serif"
        android:inputType="numberDecimal"
        android:textColorHint="@color/grey"
        android:singleLine="true"
        android:textSize="@dimen/text_head"/>
    <!--<android.support.v7.widget.AppCompatCheckBox-->
    <!--android:id="@+id/nursery"-->
    <!--android:textColor="@color/grey"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="wrap_content"-->
